{
	"name": "@travel-anchor/hono-api",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": null
		}
	},
	"imports": {
		"#shared/*": "./src/backend/shared/*",
		"#domains/*": "./src/backend/domains/*",
		"#middleware/*": "./src/backend/shared/middleware/*",
		"#utils/*": "./src/backend/shared/utils/*",
		"#database/*": "./src/backend/shared/database/*",
		"#database": "./src/backend/shared/database/index.ts",
		"#logger": "./src/backend/shared/utils/logger.ts"
	},
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"build": "bun build ./src/index.ts --outdir ./out --target node  --minify --sourcemap",
		"build:client": "bun build ./src/client-rpc.ts --outdir ./dist --target browser",
		"check": "tsc --noEmit",
		"start": "bun run ./out/index.js",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"db:generate": "prisma generate"
	},
	"dependencies": {
		"@hono/zod-openapi": "^1.1.0",
		"cheerio": "^1.1.2",
		"geolib": "^3.3.4",
		"hono": "catalog:",
		"kysely": "^0.28.5",
		"ofetch": "^1.4.1",
		"winston": "^3.17.0",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@prisma/client": "^6.16.0",
		"@types/bun": "catalog:",
		"prisma": "^6.16.0",
		"prisma-kysely": "^2.2.0",
		"typescript": "catalog:"
	}
}
